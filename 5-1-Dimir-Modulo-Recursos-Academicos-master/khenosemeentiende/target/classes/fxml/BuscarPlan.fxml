<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.stage.*?>

<Stage xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Dimir.Controller">
    <scene>
        <Scene>
            <AnchorPane prefHeight="299.0" prefWidth="461.0">
            <children>
               <Label layoutX="49.0" layoutY="136.0" prefHeight="27.0" prefWidth="173.0" text="Buscar ID plan:">
                  <font>
                     <Font name="Bookman Old Style" size="22.0" />
                  </font></Label>
               <TextField fx:id="planbuscado" layoutX="284.0" layoutY="138.0" />
               <Button layoutX="131.0" layoutY="204.0" mnemonicParsing="false" onAction="#encontrarplan" style="-fx-background-color: blue;" text="Buscar">
                  <font>
                     <Font name="Bookman Old Style" size="14.0" />
                  </font>
                  <effect>
                     <ColorAdjust brightness="-0.13" contrast="1.0" hue="-0.15" saturation="1.0" />
                  </effect></Button>
               <Button layoutX="253.0" layoutY="204.0" mnemonicParsing="false" onAction="#deletearplan" style="-fx-background-color: red;" text="Borrar">
                  <font>
                     <Font name="Bookman Old Style" size="14.0" />
                  </font>
                  <effect>
                     <ColorAdjust brightness="0.38" contrast="1.0" saturation="0.5" />
                  </effect></Button>
               <Label alignment="CENTER" prefHeight="66.0" prefWidth="461.0" style="-fx-background-color: purple;" text="Buscar Plan">
                  <font>
                     <Font name="Bookman Old Style" size="33.0" />
                  </font>
                  <effect>
                     <ColorAdjust brightness="0.38" contrast="1.0" saturation="0.5" />
                  </effect>
               </Label>
               <Button fx:id="boton_regresar" layoutX="132.0" layoutY="254.0" mnemonicParsing="false" onAction="#Regresarinicio" prefHeight="26.0" prefWidth="185.0" style="-fx-background-color: orange;" text="Regresar">
                  <effect>
                     <ColorAdjust brightness="-0.27" contrast="0.34" hue="-0.04" saturation="1.0" />
                  </effect>
                  <font>
                     <Font name="Bookman Old Style" size="14.0" />
                  </font>
               </Button>
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
